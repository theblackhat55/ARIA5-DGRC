  "name": "risk-optics-grc",
  "compatibility_date": "2025-08-14",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "risk-optics-grc-production",
